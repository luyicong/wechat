.pos_list{
  width: 100%;
  overflow: hidden;
}
.list_item{
  display: flex;
  display: -webkit-flex;
  height: 170rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
  padding:12rpx 15rpx;
  box-sizing: border-box;
}
.left{
  flex: 3;
}
.left>view{
  height: 46rpx;
  line-height: 46rpx;
  font-size:26rpx;
}
.left .title{
  /* font-weight: 600; */
  color: #000;
  font-size: 28rpx;
}
.left .tags,.com{
  color: #666;
}
.salary{
  margin-right: 10rpx;
  color: #ff552e;
}
.right{
  flex: 1;
  font-size: 24rpx;
}
.time{
  height: 66rpx;
  line-height: 66rpx;
  color: #999;
}
.btn{
  width: 80%;
  border: 2rpx solid #ff552e;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  color: #ff552e;
  text-align: center;
  margin-top: 2rpx;
}
.loading_box{
  width: 260rpx;
  height: 120rpx;
  background: #fff;
  position: fixed;
  left:50%;
  top: 50%;
  margin-top:-100rpx;
  margin-left:-130rpx;
  z-index: 1000;
  border-radius: 16rpx;
  text-align: center;
  padding-top: 40rpx;
}
.loading_item{
  background-color: #009ee5;
  width: 8rpx;
  height: 80rpx;
  border-radius: 4rpx;
  margin: 0 10rpx;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  display: inline-block;
}
.loading_box .loading_item:nth-child(1){
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
}
.loading_box .loading_item:nth-child(2){
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: line-scale 1s 0.2s infinite cubic-bezier(.2, .68, .18, 1.08);
}
.loading_box .loading_item:nth-child(3){
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: line-scale 1s 0.3s infinite cubic-bezier(.2, .68, .18, 1.08);
}
.loading_box .loading_item:nth-child(4){
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: line-scale 1s 0.4s infinite cubic-bezier(.2, .68, .18, 1.08);
}
.loading_box .loading_item:nth-child(5){
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: line-scale 1s 0.5s infinite cubic-bezier(.2, .68, .18, 1.08);
}
@keyframes line-scale {
  0% {
      -webkit-transform: scaley(1);
      transform: scaley(1);
  }

  50% {
      -webkit-transform: scaley(0.4);
      transform: scaley(0.4);
  }
  100% {
      -webkit-transform: scaley(1);
      transform: scaley(1);
  }
}