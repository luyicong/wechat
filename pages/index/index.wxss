page{
  background-color: #f5f5f5;
}
.wrap{
  height: 100%;
  overflow: hidden;
}
.slide-image{
  width: 100%;
  height: 150px;
}

.cate_wrap{
  width: 100%;
  height: 180px;
  margin-top: 15px;
  background-color: #fff;
  padding-top: 15px;
}
.cate_item{
  width: 25%;
  height: 80px;
  float: left;
  text-align: center;
  margin-bottom: 10px;
}
.cate_item .bg{
  width: 50px;
  height: 50px;
  border-radius: 25px;
  overflow: hidden;
  background-color: #ccc;
  margin:0 auto;
}
.cate_item text{
  display: inline-block;
  font-size: 14px;
}